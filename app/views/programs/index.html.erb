<p id="notice"><%= notice %></p>

<h1>Programs</h1>

<table>
  <thead>
    <tr>
      <th>Institution</th>
      <th>Program classification</th>
      <th>Credential level</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @programs.each do |program| %>
      <tr>
        <td><%= program.institution_id %></td>
        <td><%= program.program_classification_id %></td>
        <td><%= program.credential_level %></td>
        <td><%= link_to 'Show', program %></td>
        <%
=begin%>
 <td><%= link_to 'Edit', edit_program_path(program) %></td>
        <td><%= link_to 'Destroy', program, method: :delete, data: { confirm: 'Are you sure?' } %></td> 
<%
=end%>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

